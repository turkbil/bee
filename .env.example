APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost
APP_DOMAIN=laravel.test

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ===========================================
# MODÜL SİSTEM AYARLARI
# ===========================================

# Sistem Dilleri (virgülle ayrılmış)
SYSTEM_LANGUAGES=tr,en
DEFAULT_LANGUAGE=tr

# Sayfalama Ayarları
ADMIN_PER_PAGE=10
FRONT_PER_PAGE=12

# Cache Ayarları
MODULE_CACHE_ENABLED=true
CACHE_TTL_LIST=3600
CACHE_TTL_DETAIL=7200
CACHE_TTL_API=1800

# Response Cache
RESPONSE_CACHE_ENABLED=true
RESPONSE_CACHE_DRIVER=redis
RESPONSE_CACHE_LIFETIME=3600

# Queue Ayarları
MODULE_QUEUE_CONNECTION=redis
MODULE_QUEUE_NAME=tenant_isolated
QUEUE_TIMEOUT=300
QUEUE_TRIES=3
QUEUE_RETRY_AFTER=90

# Medya Ayarları
MEDIA_MAX_FILE_SIZE=10240

# ===========================================
# AI PROVIDER API KEYS
# ===========================================
# NOT: API key'ler admin panelden de yönetilebilir
# Database'de şifrelenmiş olarak saklanır
# Bu ayarlar sadece fallback ve ilk kurulum içindir

OPENAI_API_KEY=
ANTHROPIC_API_KEY=
DEEPSEEK_API_KEY=

# ===========================================
# PHP AYARLARI (Plesk için)
# ===========================================
PHP_MEMORY_LIMIT=512M
PHP_MAX_EXECUTION_TIME=300
PHP_MAX_INPUT_TIME=300
HTTP_TIMEOUT=3600
